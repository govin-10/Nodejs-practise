    ul {
      display: flex;
    }
    ul li {
      list-style: none;
      margin-right: 2rem;
      font-size: 1.2rem;
    }
    a {
      text-decoration: none;
      color: white;
    }
  </style>
  <body>
    <div class="width-full">
      <nav>
        <ul>
          <li><a href="">Home</a></li>
          <li><a href="">Blogs</a></li>
